<div class="content-header">
  <h1 class="title">
    <a id="menu-toggle" href="#" class="btn btn-default"><i class="fa fa-bars"></i></a>
    Visitors on your page
  </h1>
</div>

<!-- Keep all page content within the page-content inset div! -->
<div class="page-content inset" ng-app="chatApp">
  <div class="row">
    <div class="col-md-12">
      <table id="chatUserTable" ng-controller="ChatCtrl">
        <tr>
          <th>#</th>
          <th>Visitor</th>
          <th>Viewing</th>
        </tr>
        <tr ng-repeat="(id, lead) in leads">
          <td>{{lead.online}}</td>
          <td>{{id}}</td>
          <td>urbanzeak.com/experiences/somepage</td>
        </tr>
      </table>
    </div>
  </div>

</div>




