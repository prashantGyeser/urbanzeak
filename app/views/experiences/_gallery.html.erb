<!--GALLERY START-->
<div class="container">
  <!--MARGIN TOP AND BOTTOM START-->
  <div class="p-t-20" id="experience_pictures">
    <!--<h2 class="p-t-10">Experience <span class="semi-bold">Pictures</span></h2>-->
    <h1 class="p-t-10 center-text semi-bold"><%= @experience.name %></h1>
    <h4 class="center-text semi-bold p-b-10"><%= @experience.tagline %></h4>
    <div class="center-text p-b-40">
      <a href="#" class="btn btn-purple btn-lg  btn-large m-r-10 center-block" style="-webkit-transition: all 0.12s linear 0s; transition: all 0.12s linear 0s; min-height: 0px; min-width: 0px; white-space: nowrap; line-height: 15px; border-width: 0px; margin: 0px 7.68px 11.52px 0px; padding: 12px 19px; font-size: 15px;">BUY IT NOW</a>
    </div>

    <div class="portfolio-grid portfolioContainer ">
      <ul id="thumbs" class="col3 isotope" style="position: relative; overflow: hidden; height: 1406px">

        <% @images.each do |image| %>
          <li class="item web isotope-item" style="position: absolute; left: 0px; top: 0px; -webkit-transform: translate3d(0px, 0px, 0px);">
            <div class="portfolio-image-wrapper" style="height: 271px;">
              <%# image_tag image.image_url.to_s %>
              <%= filepicker_image_tag image.image, w: 600, h: 600, fit: 'crop', style: "margin-bottom: 10px" %>
            </div>
            <div class="clearfix"></div>
          </li>
        <% end %>

      </ul>
    </div>
  </div>
  <!--MARGIN TOP AND BOTTOM END-->
</div>
<br>
<br>
<!--GALLERY END-->
