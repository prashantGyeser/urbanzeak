  <!--JAVASCRIPT INCLUDES END-->


</head>
<!-- END HEAD -->
<body>
<div class="main-wrapper">

<div class="section ha-waypoint"  data-animate-down="ha-header-hide" data-animate-up="ha-header-hide">

  <!--BEGIN SLIDER -->
  <div class="tp-banner-container">
    <div class="tp-banner" id="home" >
      <!-- SLIDE  -->
      <ul>

        <% @header_images.each do |image| %>
          <!--INVIDIDUAL SLIDE START -->
          <li data-transition="fade" data-slotamount="5" data-masterspeed="700" >
            <!-- MAIN IMAGE -->
            <%= image_tag image.image_url.to_s, :data => { :bgfit => "cover", :bgposition => "center center", :bgrepeat => "no-repeat"  } %>

            <!-- LAYERS -->
            <div class="tp-caption mediumlarge_light_white_center sft tp-resizeme slider"
                 data-x="center" data-hoffset="0"
                 data-y="60"
                 data-speed="500"
                 data-start="800"
                 data-easing="Power4.easeOut"
                 data-endspeed="300"
                 data-endeasing="Power1.easeIn"
                 data-captionhidden="off"
                 style="z-index: 6">
              <h2 class="text-white custom-font title "><%= @experience.name %></h2>

            </div>
            <div class="tp-caption sfb slider tp-resizeme slider"
                 data-x="center"
                 data-hoffset="0"
                 data-y="240"
                 data-speed="800"
                 data-start="1000"
                 data-easing="Power4.easeOut"
                 data-endspeed="300"
                 data-endeasing="Power1.easeIn"
                 data-captionhidden="off"
                 style="z-index: 6">
            </div>

          </li>

          <!--INVIDIDUAL SLIDE END -->
        <% end %>
        <!-- SLIDE  -->

      </ul>
      <div class="tp-bannertimer"></div>
    </div>
  </div>
  <!--END SLIDER

  -->



</div>
<div class="section ">
  <div class="container">
    <div class="p-t-40 p-b-40">
      <h2 class="text-center"><%= @experience.name %></h2>
    </div>
  </div>
</div>
<div class="section white ha-waypoint"  data-animate-down="ha-header-color" data-animate-up="ha-header-hide" >
  <div class="container">

    <div class="p-t-60 p-b-60">
      <div class="row">
        <div class="col-md-6 feature-list ">
          <h2 class="normal"><span class="semi-bold">About </span> the experience</h2>
          <%= simple_format(@experience.description) %>

        </div>
        <div class="col-md-6 col-vlg-4 col-sm-12 ">
          <h3><span class="semi-bold">Join </span>now</h3>

          <%= render partial: 'attendees/form', :locals => {:experience_id => @experience.id} %>

        </div>

      </div>
      <br>
      <div class="clearfix"></div>
    </div>

  </div>
</div>


<!--GALLERY START-->
<div class="container">
  <!--MARGIN TOP AND BOTTOM START-->
  <div class="p-t-60 p-b-80">
    <div class="portfolio-grid portfolioContainer ">
      <ul id="thumbs" class="col3 isotope" style="position: relative; overflow: hidden; height: 1406px;">

        <% @images.each do |image| %>
          <li class="item web isotope-item" style="position: absolute; left: 0px; top: 0px; -webkit-transform: translate3d(0px, 0px, 0px);">
            <div class="portfolio-image-wrapper" style="height: 271px;">
              <%= image_tag image.image_url.to_s %>
            </div>
            <div class="clearfix"></div>
          </li>
        <% end %>

      </ul>
    </div>
  </div>
  <!--MARGIN TOP AND BOTTOM END-->
</div>
<br>
<br>
<!--GALLERY END-->

